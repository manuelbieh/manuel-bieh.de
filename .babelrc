{
    "presets": [
        [
            "@babel/env",
            {
                "loose": true,
                "modules": false,
                "shippedProposals": true,
                "targets": {
                    "browsers": [">0.25%", "not dead"]
                }
            }
        ],
        "@babel/react",
        "@babel/typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        "@babel/plugin-syntax-dynamic-import",
        [
            "@babel/plugin-transform-runtime",
            {
                "helpers": true,
                "regenerator": true
            }
        ]
    ]
}
