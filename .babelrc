{
    "presets": [
        [
            "@babel/env",
            {
                "loose": true,
                "modules": false,
                "shippedProposals": true
            }
        ],
        "@babel/react",
        "@babel/typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        "@babel/plugin-syntax-dynamic-import",
        "babel-plugin-macros",
        [
            "@babel/plugin-transform-runtime",
            {
                "helpers": true,
                "regenerator": true
            }
        ]
    ]
}
